{
  "name": "<%= _.slugify(appname) %>",
  "devDependencies": {
    "apache-server-configs": "~2.7.1",<% if (includeHandlebars) { %>
    "assemble": "~0.4.42",<% } %>
    "grunt": "~0.4.5",
    "grunt-autoprefixer": "~3.0.0",
    "grunt-concurrent": "~1.0.0",
    "grunt-browser-sync": "~2.1.1",
    "grunt-contrib-clean": "~0.6.0",<% if (coffee) { %>
    "grunt-contrib-coffee": "~0.12.0",<% } %>
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-copy": "~0.7.0",
    "grunt-contrib-cssmin": "~0.11.0",
    "grunt-contrib-htmlmin": "~0.3.0",
    "grunt-contrib-imagemin": "~0.9.2",<% if (testFramework === 'jasmine') { %>
    "grunt-contrib-jasmine": "~0.8.1",<% } %>
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-uglify": "~0.7.0",
    "grunt-contrib-watch": "~0.6.1",<% if (testFramework === 'mocha') { %>
    "grunt-mocha": "~0.4.11",<% } %><% if (includeModernizr) { %>
    "grunt-modernizr": "~0.6.0",<% } %>
    "grunt-newer": "~1.1.0",
    "grunt-rev": "~0.1.0",<% if (includeSass) { %>
    "grunt-sass": "~0.18.0",<% } %><% if (includeSprites) { %>
    "grunt-spritesmith": "~3.9.0",<% } %>
    "grunt-svgmin": "~2.0.0",
    "grunt-usemin": "~3.0.0",
    "grunt-wiredep": "~2.0.0",
    "jshint-stylish": "~1.0.0",
    "load-grunt-tasks": "~2.0.0",
    "time-grunt": "~1.0.0"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
